{
    "name": "leewillis77/cached-embed",
    "description": "Laravel package to cache embed information from https://github.com/oscarotero/Embed. Works with Laravel 5.5 to 5.8",
    "keywords": [
        "Laravel",
	"oembed",
        "embed",
        "cache"
    ],
    "homepage": "https://github.com/leewillis77/cached-embed",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Willis",
            "homepage": "https://www.leewillis.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "embed/embed": "~3.3",
        "illuminate/cache": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Leewillis77\\CachedEmbed\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leewillis77\\CachedEmbed\\CachedEmbedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    }
}
